{
  "name": "simple-app",
  "version": "1.0.0",
  "description": "Simple APP for register, login, logout and managing todos",
  "repository": {
    "url": "https://github.com/mitevs/simple-api",
    "type": "git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node src/index.ts",
    "start": "cross-env NODE_ENV=production ts-node --transpile-only src/index.ts",
    "test": "alsatian 'tests/**/*.spec.ts'",
    "up": "docker-compose up -d --build",
    "down": "docker-compose down"
  },
  "author": "Stefan Mitev <stefomitev@gmail.com>",
  "dependencies": {
    "apollo-server": "^2.19.2",
    "apollo-server-koa": "^2.19.2",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-multer": "^1.0.2",
    "koa-router": "^10.0.0",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "ts-node": "^9.1.1",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.30",
    "typeorm-typedi-extensions": "^0.4.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.14.21",
